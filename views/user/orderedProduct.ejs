<%- include('../partials/user_header')%>

    <!-- breadcrumb-section -->
    <div class="breadcrumb-section breadcrumb-bg">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 offset-lg-2 text-center">
                    <div class="breadcrumb-text">

                        <h1>Order details</h1>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end breadcrumb section -->


    <!-- Middle section -->
    <section class="h-100 gradient-custom">
        <div class="container py-5 h-100">
            <div class="row d-flex justify-content-center align-items-center h-100">
                <div class="col-lg-10 col-xl-8">
                    <div class="card mt-3" style="border-radius: 10px">
                        <div class="card-header px-4 py-5">
                            <h5 class="text-muted mb-0">
                                Product Details,
                                <span style="color: #a8729a"> </span>!
                            </h5>
                        </div>
                        <div class="card-body p-4">
                            <div class="d-flex justify-content-between align-items-center mb-4">
                                <p class="lead fw-normal mb-0" style="color: #a8729a">
                                    Receipt
                                </p>
                                <p class="small text-muted mb-0">
                                    Receipt Voucher : 1KAU9-84UIL
                                </p>
                            </div>

                            <div class="card shadow-0 border mb-4">
                                <div class="card-body">
                                    <% productData.forEach((order)=>{ %>
                                        <div class="row">
                                            <div class="col-md-2">
                                                <img src="/adminimages/<%= order.productDetail._id %>.jpg" class="img-fluid" alt="" />
                                            </div>
                                        
                                            <div
                                                class="col-md-2 text-center d-flex justify-content-center align-items-center">
                                                <p class="text-muted mb-0">
                                                    <%= order.productDetail.product_name %>

                                                </p>
                                            </div>
                                            
                                            <div
                                                class="col-md-2 text-center d-flex justify-content-center align-items-center">
                                                <p class="text-muted mb-0 small">
                                                    <%= order.productDetail.description %>

                                                </p>
                                            </div>
                                            <div
                                                class="col-md-2 text-center d-flex justify-content-center align-items-center">
                                                <p class="text-muted mb-0 small">
                                                    <%= order.productQuantity %> kg

                                                </p>
                                            </div>
                                            <div
                                                class="col-md-2 text-center d-flex justify-content-center align-items-center">
                                                <p class="text-muted mb-0 small">
                                                    <%= order.productDetail.price %>/kg

                                                </p>
                                            </div>
                                            <div
                                            <% var a=order.productQuantity%>
											    <% var b=order.productDetail.price%>
												<% var c=a*b %>
                                            class="col-md-2 text-center d-flex justify-content-center align-items-center">
                                            <p class="text-muted mb-0 small">
                                                
                                                <%=c %> 

                                            </p>
                                        </div>
                                        </div>
                                        <br>
                                        <br>
                                     <% }) %>

                                            <hr class="mb-4" style="background-color: #e0e0e0; opacity: 1" />
                                            <div class="row d-flex align-items-center">
                                                <div class="col-md-2">
                                                    <p class="text-muted mb-0 small">Track Order</p>
                                                </div>
                                                <div class="col-md-10">
                                                    <div class="progress" style="height: 6px; border-radius: 16px">
                                                        <div class="progress-bar" role="progressbar" style="
                                      width: 5%;
                                      border-radius: 16px;
                                      background-color: #a8729a;
                                    " aria-valuenow="65" aria-valuemin="0" aria-valuemax="100"></div>
                                                    </div>
                                                    <div class="d-flex justify-content-around mb-1">
                                                        <p class="text-muted mt-1 mb-0 small ms-xl-5">
                                                            Out for delivary
                                                        </p>
                                                        <p class="text-muted mt-1 mb-0 small ms-xl-5">
                                                            Delivered
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--End Middle section -->

    <!-- footer -->
    <%- include('../partials/user_footer')%>
        <!-- end footer -->



        <!-- jquery -->
        <script src="http://localhost:5000/assets/js/jquery-1.11.3.min.js"></script>
        <!-- bootstrap -->
        <script src="http://localhost:5000/assets/bootstrap/js/bootstrap.min.js"></script>
        <!-- count down -->
        <script src="http://localhost:5000/assets/js/jquery.countdown.js"></script>
        <!-- isotope -->
        <script src="http://localhost:5000/assets/js/jquery.isotope-3.0.6.min.js"></script>
        <!-- waypoints -->
        <script src="http://localhost:5000/assets/js/waypoints.js"></script>
        <!-- owl carousel -->
        <script src="http://localhost:5000/assets/js/owl.carousel.min.js"></script>
        <!-- magnific popup -->
        <script src="http://localhost:5000/assets/js/jquery.magnific-popup.min.js"></script>
        <!-- mean menu -->
        <script src="http://localhost:5000/assets/js/jquery.meanmenu.min.js"></script>
        <!-- sticker js -->
        <script src="http://localhost:5000/assets/js/sticker.js"></script>
        <!-- main js -->
        <script src="http://localhost:5000/assets/js/main.js"></script>

        </body>

        </html>